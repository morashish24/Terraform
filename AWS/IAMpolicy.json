{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "allow",
            "Action": "s3:GetObject",       
            "Resource": "arn:aws:s3::my-example-bucket/*"
        },
        {
            "Effect": "Allow",
            "Action": "s3:PutObject",
            "Resource": "arn:aws:s3::my-example-bucket/*"
        }
    ]
}